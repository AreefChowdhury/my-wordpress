					"fontFamily": "var(--wp--preset--font-family--eb-garamond)",
					"fontSize": "var(--wp--custom--typography--font-size--colossal)",
					"fontWeight": "500"
				}
			},
			"core/query-title": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--eb-garamond)",
					"fontWeight": "500"
				}
			},
			"core/site-title": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--eb-garamond)",
