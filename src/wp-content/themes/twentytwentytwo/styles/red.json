{
	"version": 2,
	"title": "Red",
	"settings": {
		"color": {
			"palette": [
				{
					"slug": "foreground",
					"color": "#6A0909",
					"name": "Foreground"
				},
				{
					"slug": "background",
					"color": "#ffffff",
					"name": "Background"
				},
				{
					"slug": "primary",
					"color": "#000000",
					"name": "Primary"
				},
				{
					"slug": "secondary",
					"color": "#FCF5ED",
					"name": "Secondary"
				},
				{
					"slug": "tertiary",
					"color": "#FAE5CD",
					"name": "Tertiary"
				}
			]
		},
		"custom": {
			"spacing": {
				"small": "max(1.25rem, 5vw)",
				"medium": "clamp(2rem, 8vw, calc(4 * var(--wp--style--block-gap)))",
				"large": "clamp(4rem, 10vw, 8rem)",
				"outer": "var(--wp--custom--spacing--small, 1.25rem)"
			},
			"typography": {
				"font-size": {
					"huge": "clamp(2.75rem, 4vw, 3rem)",
					"gigantic": "clamp(3rem, 6vw, 3.25rem)",
					"colossal": "clamp(3.25rem, 8vw, 6rem)"
				}
			}
		},
		"layout": {
			"contentSize": "675px",
			"wideSize": "1160px"
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "\"EB Garamond\", serif",
					"name": "EB Garamond",
					"slug": "eb-garamond",
					"fontFace": [
						{
							"fontFamily": "EB Garamond",
							"fontWeight": "400",
							"fontStyle": "normal",
							"src": [ "file:./assets/fonts/dm-sans/EBGaramond12-Regular.woff" ]
						},
						{
							"fontFamily": "EB Garamond",
							"fontWeight": "400",
							"fontStyle": "italic",
							"src": [ "file:./assets/fonts/dm-sans/EBGaramond12-Italic.woff" ]
						}
					]
				}
			]
		}
	},
